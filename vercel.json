{
	"headers": [
		{
			"source": "/api/public/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, s-maxage=7200, stale-while-revalidate=600"
				}
			]
		},
		{
			"source": "/static/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
